function notSupported()
	OrionLib:MakeNotification({
		Name = "ðŸ‘Ž L-Hub ðŸ‘Ž",
		Content = "The game is not supported!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end
 
